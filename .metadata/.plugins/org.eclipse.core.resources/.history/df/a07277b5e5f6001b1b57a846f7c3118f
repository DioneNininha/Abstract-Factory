package app;

import objetos.FabricaYamaha;
import objetos.FabricaRoland;
/**
 * 
 * @author Dione
 *
 */
public class Main {

	public static void main(String[] args) {
		
		System.out.println("******************************");
		System.out.println("<<<<<<< FABRICA YAMAHA >>>>>>>\n");
	

		FabricaYamaha yamaha = new FabricaYamaha();

		yamaha.fabricarSom().exibir();
		yamaha.fabricarTeclado().exibir();
		
		System.out.println("\n******************************");
		System.out.println("<<<<<<<<< FÁBRICA ROLAND >>>>>>>>\n");
	

		FabricaRoland roland = new FabricaRoland();

		roland.fabricarSom().exibir();
		roland.fabricarTeclado().exibir();
	}
}
